                {{#each events}}
                    <tr>
                        <td><a href="events/{{event_id}}">{{event_name}}</a></td>
                        <td><a href="venues/{{venue_id}}">{{venue_name}}</a></td>
                        <td>{{#dateTimeString date_and_time}} {{/dateTimeString}}</td>
                        <td><a href="{{ticket_link}}">Link</a></td>
                    </tr>
                {{/each}}
